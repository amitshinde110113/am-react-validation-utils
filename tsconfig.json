{
    "compilerOptions": {
      "outDir": "./dist",
      "rootDir": "./src",
      "target": "ES6", // or "ES2016" or later
      "module": "CommonJS",
      "lib": ["ES2016", "DOM"], // Add ES2016 or later
      "declaration": true,
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*"]
  }
  